<!-- 다중 조건 chosse when otherwise -->

<c:set var="name" value="JAVA"/>
<c:choose>
	<c:when test="${name eq 'AVA'}">
		JAVA 입니다.
	</c:when>
	<c:otherwise>
		해당되는  과목이 없습니다.
	</c:otherwise>
</c:choose><br>
<%
	String name=request.getParameter("name");
	String age=request.getParameter("age");
	
	

%>

이름: <%=name %>,나이: <%=age %><br>
이름: ${param.name },나이: ${param.age }<br> <!-- 파라미터값 -->

<c:choose>
	<c:when test="${(param.name eq 'm1') and (param.age eq '11') }">
		로그인성공	
	</c:when>
	<c:otherwise>
		로그인실패
	</c:otherwise>

</c:choose>
<br>
<!-- 요청 url을 처리 import -->
<% request.setCharacterEncoding("uft-8"); %>
<c:choose>
	<c:when test="${param.type eq 'naver' }">
		<c:import url="http://www.naver.com/"/>
	</c:when>
	<c:when test="${param.type eq 'daum' }">
		<c:import url="http://www.daum.com/"/>
	</c:when>
	<c:when test="${param.type eq 'java' }">
		<c:import url="http://www.java.com/"/>
	</c:when>
	<c:otherwise>
		<c:import url="http://www.google.com/"/>
	</c:otherwise>


</c:choose>


</body>
</html>